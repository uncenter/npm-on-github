		"format": "prettier --write ."
	},
	"license": "MIT",
	"dependencies": {
		"chart.js": "^4.3.3",
		"pkg-types": "^1.0.3"
	},
	"devDependencies": {
		"@types/chrome": "^0.0.243",
		"esbuild": "^0.19.2",
		"prettier": "^3.0.2",
		"rimraf": "^5.0.1",
		"vitest": "^0.34.2"
	},
	"packageManager": "pnpm@8.6.12"
}
