{
    "manifest_version": 3,
    "name": "NPM on Github",
    "version": "1.0",
    "description": "View NPM package downloads for Github repositories",
    "icons": {
        "16": "icons/icon16.png",
        "48": "icons/icon48.png",
        "128": "icons/icon128.png"
    },
    "content_scripts": [
        {
            "matches": ["https://github.com/*"],
            "js": ["js/vendor.js", "js/content_script.js"],
            "css": ["styles.css"]
        }
    ]
}
